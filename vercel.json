{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_FIREBASE_API_KEY": {
      "description": "Firebase API Key (from firebaseConfig.apiKey)"
    },
    "NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN": {
      "description": "Firebase Auth Domain (from firebaseConfig.authDomain)"
    },
    "NEXT_PUBLIC_FIREBASE_PROJECT_ID": {
      "description": "Firebase Project ID (from firebaseConfig.projectId)"
    },
    "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": {
      "description": "Firebase Storage Bucket (from firebaseConfig.storageBucket)"
    },
    "NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": {
      "description": "Firebase Messaging Sender ID (from firebaseConfig.messagingSenderId)"
    },
    "NEXT_PUBLIC_FIREBASE_APP_ID": {
      "description": "Firebase App ID (from firebaseConfig.appId)"
    }
  }
}
